        "./es6/for..of.ts",
        "./es6/let.ts",
        "./es6/rest-parameters.ts",
        "./es6/spread-operator.ts",
        "./es6/template-strings.ts",
        "./es6/test.ts",
        "./testing.ts",
        "./tips/mixins.ts",
        "./types/lib/exclude/nolibd.ts",
        "./types/lib/usage/libd.ts",
        "./types/types.ts"
    ]
}
